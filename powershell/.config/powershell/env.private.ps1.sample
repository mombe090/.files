# ===== CLOUD CREDENTIALS =====
# Add your cloud provider credentials here

# AWS
# $env:AWS_ACCESS_KEY_ID = "YOUR_AWS_ACCESS_KEY_ID"
# $env:AWS_SECRET_ACCESS_KEY = "YOUR_AWS_SECRET_ACCESS_KEY"
# $env:AWS_ACCOUNT_ID = "123456789012"

# Azure
# $env:AZURE_TENANT_ID = "YOUR_AZURE_TENANT_ID"
# $env:AZDO_PERSONAL_ACCESS_TOKEN = "YOUR_AZDO_PAT_TOKEN"

# Google Cloud
# $env:GOOGLE_CLOUD_PROJECT = "your-gcp-project-id"
# $env:GEMINI_API_KEY = "YOUR_GEMINI_API_KEY"

# ===== API KEYS & TOKENS =====
# Add your API keys and tokens here

# GitHub
# $env:GITHUB_TOKEN = "YOUR_GITHUB_PAT_TOKEN"

# OpenAI
# $env:OPENAI_API_KEY = "YOUR_OPENAI_API_KEY"

# Anthropic Claude
# $env:ANTHROPIC_API_KEY = "YOUR_ANTHROPIC_API_KEY"

# ===== INFRASTRUCTURE CREDENTIALS =====
# Add your infrastructure credentials here

